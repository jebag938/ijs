<%# --- Skrip Slider Otomatis dan Widget Populer (Hanya untuk homepage) --- %>
<% if (page === 'home' || page === 'jadwal') { %>
  <script>
    document.addEventListener('DOMContentLoaded', () => {
      function slugify(text) {
        if (!text) return '';
        return text.toString().toLowerCase().trim()
          .replace(/\s+/g, '-') 
          .replace(/[^\w\-]+/g, '')
          .replace(/\-\-+/g, '-'); 
      }

      const sliderContainer = document.querySelector('.slider-container');
      if (sliderContainer) {
        let scrollAmount = 0;
        const scrollInterval = 3000;
        let slideWidth = 300;
        const calculateSlideWidth = () => {
          const firstSlide = sliderContainer.querySelector('.slide');
          if (firstSlide) {
            const slideStyle = window.getComputedStyle(firstSlide);
            slideWidth = firstSlide.offsetWidth + parseFloat(slideStyle.marginRight || 0);
          }
        };
        calculateSlideWidth();
        window.addEventListener('resize', calculateSlideWidth);
        setInterval(() => {
          scrollAmount += slideWidth;
          if (scrollAmount >= (sliderContainer.scrollWidth - sliderContainer.clientWidth - slideWidth / 2)) {
            sliderContainer.scrollTo({ left: 0, behavior: 'smooth' });
            scrollAmount = 0;
          } else {
            sliderContainer.scrollTo({ left: scrollAmount, behavior: 'smooth' });
          }
        }, scrollInterval);
      }

      const popularTabs = document.querySelectorAll('.ts-wpop-tab');
      const popularItemsContainer = document.getElementById('wpop-items');

      if (popularTabs.length > 0 && popularItemsContainer) {
        const loadPopularData = async (range = 'weekly') => {
          popularItemsContainer.innerHTML = `
            <div class="serieslist pop wpop loading-popular">
              <p>Memuat data populer...</p>
            </div>`;
          try {
            const response = await fetch(`/api/popular?range=${range}`);
            if (!response.ok) throw new Error('Network response was not ok');
            const animes = await response.json();
            if (animes.length === 0) {
              popularItemsContainer.innerHTML = `
                <div class="serieslist pop wpop">
                  <ul><li><p style="padding: 10px 0; color: #888;">Tidak ada data populer untuk ditampilkan.</p></li></ul>
                </div>`;
              return;
            }
            let listHtml = `<div class="serieslist pop wpop wpop-${range}"><ul>`;
            animes.forEach((anime, index) => {
              const genres = (anime.genres && anime.genres.length > 0)
                ? anime.genres.slice(0, 2).map(g => `<a href="/genre/${slugify(g)}">${g}</a>`).join(', ')
                : 'N/A';
              const safePageSlug = encodeURIComponent(anime.pageSlug);
              listHtml += `
                <li>
                  <div class="ctr">${index + 1}</div>
                  <div class="imgseries">
                    <a class="series" href="/anime/${safePageSlug}">
                      <img src="${anime.imageUrl || '/images/default.jpg'}" class="ts-post-image" loading="lazy" alt="${anime.title}" width="45" height="65">
                    </a>
                  </div>
                  <div class="leftseries">
                    <h4><a class="series" href="/anime/${safePageSlug}">${anime.title}</a></h4>
                    <span><b>Genres</b>: ${genres}</span>
                  </div>
                </li>
              `;
            });
            listHtml += '</ul></div>';
            popularItemsContainer.innerHTML = listHtml;
          } catch (error) {
            console.error('Error fetching popular data:', error);
            popularItemsContainer.innerHTML = `
              <div class="serieslist pop wpop">
                <ul><li><p style="padding: 10px 0; color: #d9534f;">Gagal memuat data populer.</p></li></ul>
              </div>`;
          }
        };
        popularTabs.forEach(tab => {
          tab.addEventListener('click', () => {
            popularTabs.forEach(t => t.parentElement.classList.remove('active'));
            tab.parentElement.classList.add('active');
            const range = tab.getAttribute('data-range');
            loadPopularData(range);
          });
        });
        loadPopularData('weekly');
      }
    }); // Akhir DOMContentLoaded
  </script>
<% } %>
<% if (page === 'jadwal') { %>
<script>
  document.addEventListener('DOMContentLoaded', async () => {
    const grid = document.getElementById('jadwal-grid');
    const loading = document.getElementById('jadwal-loading');
    
    const JIKAN_API_URL = 'https://api.jikan.moe/v4/anime?genres=12&order_by=start_date&sort=desc&limit=24';

    try {
      const response = await fetch(JIKAN_API_URL);
      if (!response.ok) {
        throw new Error(`Jikan API error: ${response.status}`);
      }
      const data = await response.json();

      if (!data.data || data.data.length === 0) {
        loading.innerHTML = '<p>Tidak ada data jadwal yang ditemukan.</p>';
        return;
      }

      loading.style.display = 'none';
      grid.innerHTML = ''; 

      data.data.forEach(anime => {
        const card = document.createElement('div');
        card.className = 'card';
        
        const title = anime.title_english || anime.title;
        const imageUrl = anime.images?.jpg?.image_url || '/images/default.jpg';
        
        let releaseDate = 'N/A';
        if (anime.aired?.from) {
          try {
            releaseDate = new Date(anime.aired.from).toLocaleDateString('id-ID', {
              year: 'numeric', month: 'long', day: 'numeric'
            });
          } catch (e) {
            releaseDate = anime.aired.string || 'N/A';
          }
        }
        card.innerHTML = `
          <a href="/search?q=${encodeURIComponent(title)}">
            <img src="${imageUrl}" alt="${title}" loading="lazy">
            <h3>${title}</h3>
          </a>
          
          <div class="card-info" style="padding: 0 10px 10px 10px; font-size: 0.85em; color: var(--text-secondary);">
            <strong>Rilis:</strong> ${releaseDate} <br>
            <strong>Status:</strong> ${anime.status}
          </div>
        `;
       
        
        grid.appendChild(card);
      });

    } catch (error) {
      console.error('Gagal mengambil jadwal Jikan:', error);
      loading.innerHTML = '<p style="color: #d9534f;">Gagal memuat data jadwal. Silakan coba lagi nanti.</p>';
    }
  });
</script>
<% } %>
