<script type="text/javascript">
  function applyTheme(theme) {
    const themeToggle = document.getElementById('lightdark');
    const icon = document.getElementById('sld');
    document.documentElement.setAttribute('data-theme', theme);
    if (theme === 'dark') {
      if (themeToggle) themeToggle.checked = true;
      if (icon) {
        icon.classList.remove('fa-sun-o');
        icon.classList.add('fa-moon-o');
      }
    } else {
      if (themeToggle) themeToggle.checked = false;
      if (icon) {
        icon.classList.remove('fa-moon-o');
        icon.classList.add('fa-sun-o');
      }
    }
  }
  // Jalankan segera untuk menghindari FOUC (Flash of Unstyled Content)
  const savedThemeOnLoad = localStorage.getItem('theme') || 'light'; // Tentukan default 'light'
  applyTheme(savedThemeOnLoad);

  document.addEventListener('DOMContentLoaded', function () {
    // Terapkan lagi jika tema default dari OS berbeda (opsional)
    // const savedTheme = localStorage.getItem('theme') || (window.matchMedia('(prefers-color-scheme: dark)').matches ? 'dark' : 'light');
    // applyTheme(savedTheme);

    const toggle = document.getElementById('lightdark');
    if (toggle) {
      toggle.addEventListener('change', function () {
        const newTheme = this.checked ? 'dark' : 'light';
        applyTheme(newTheme);
        localStorage.setItem('theme', newTheme);
      });
    }
  });
</script>


<%# --- Skrip Nonton --- %>
<% if (page === 'nonton') { %>
  <script>
    document.addEventListener('DOMContentLoaded', () => {
      const player = document.getElementById('videoPlayer');
      const streamButtons = document.querySelectorAll('.stream-btn');
      const downloadLinks = document.querySelectorAll('.download-link');

      // Logika Ganti Stream
      streamButtons.forEach(button => {
        button.addEventListener('click', () => {
          const encodedUrl = button.getAttribute('data-url');
          if (player && encodedUrl) {
            try {
              const decodedUrl = atob(encodedUrl);
              player.src = decodedUrl;
            } catch (e) {
              console.error("Gagal decode URL stream:", encodedUrl, e);
              alert("Terjadi kesalahan saat memuat server stream.");
            }
          }
          streamButtons.forEach(btn => btn.classList.remove('active'));
          button.classList.add('active');
        });
      });

      // Logika Link Download
      downloadLinks.forEach(link => {
        link.addEventListener('click', (event) => {
          event.preventDefault();
          const encodedUrl = link.getAttribute('href');
          if (encodedUrl) {
            try {
              const decodedUrl = atob(encodedUrl);
              window.open(decodedUrl, '_blank');
            } catch (e) {
              console.error("Gagal decode URL download:", encodedUrl, e);
              alert("Terjadi kesalahan saat membuka link download.");
            }
          }
        });
      });
    }); // Akhir DOMContentLoaded (Nonton)
  </script>
<% } %>


<%# --- Skrip Load More (Hanya untuk halaman list non-utama) --- %>
<% if (page === 'list') { %>
  <script>
    document.addEventListener('DOMContentLoaded', () => {
      const loadMoreBtn = document.getElementById('loadMoreBtn');
      const animeGrid = document.getElementById('anime-grid');

      if (loadMoreBtn && animeGrid) {
        let currentPage = parseInt(loadMoreBtn.dataset.currentpage);
        const totalPages = parseInt(loadMoreBtn.dataset.totalpages);
        let apiUrl = loadMoreBtn.dataset.apiurl;

        loadMoreBtn.addEventListener('click', async () => {
          currentPage++;
          loadMoreBtn.disabled = true;
          loadMoreBtn.textContent = 'Loading...';
          try {
            const separator = apiUrl.includes('?') ? '&' : '?';
            const response = await fetch(`${apiUrl}${separator}page=${currentPage}`);
            const newAnimes = await response.json();
            if (newAnimes.length > 0) {
              newAnimes.forEach(anime => {
                const card = document.createElement('div');
                card.className = 'card';
                card.innerHTML = `
                  <a href="/anime/${anime.pageSlug}">
                    <img src="${anime.imageUrl}" alt="${anime.title}">
                    <h3>${anime.title}</h3>
                  </a>
                `;
                animeGrid.appendChild(card);
              });
              loadMoreBtn.dataset.currentpage = currentPage;
              loadMoreBtn.disabled = false;
              loadMoreBtn.textContent = 'Load More';
            } else {
              loadMoreBtn.textContent = 'Selesai';
              loadMoreBtn.disabled = true;
            }
            if (currentPage >= totalPages) {
              loadMoreBtn.style.display = 'none';
            }
          } catch (error) {
            console.error('Error fetching more anime:', error);
            loadMoreBtn.textContent = 'Error. Coba lagi.';
            loadMoreBtn.disabled = false;
            currentPage--;
          }
        });
      }
    }); // Akhir DOMContentLoaded (List)
  </script>
<% } %>


<%# --- Skrip Bookmark (Hanya untuk halaman anime detail) --- %>
<% if (page === 'anime') { %>
  <script>
    document.addEventListener('DOMContentLoaded', () => {
      const bookmarkBtn = document.getElementById('bookmarkBtn');
      if (!bookmarkBtn) return;
      const animeId = bookmarkBtn.dataset.animeid;
      if (!animeId) { console.error('Anime ID not found on bookmark button.'); return; }

      let userId = localStorage.getItem('nekopoiUserId');
      if (!userId) { userId = 'user_' + Date.now() + Math.random().toString(36).substring(2); localStorage.setItem('nekopoiUserId', userId); }
      let isBookmarked = false;

      const checkStatus = async () => {
        try {
          const response = await fetch(`/api/bookmark-status?userId=${userId}&animeId=${animeId}`);
          if (!response.ok) throw new Error(`HTTP error! status: ${response.status}`);
          const result = await response.json();
          isBookmarked = result.isBookmarked;
          updateButton();
        } catch (error) { console.error('Gagal cek status bookmark:', error); }
      };

      const updateButton = () => {
        if (isBookmarked) {
          bookmarkBtn.textContent = 'âœ“ Bookmarked';
          bookmarkBtn.classList.add('bookmarked');
        } else {
          bookmarkBtn.textContent = 'Bookmark';
          bookmarkBtn.classList.remove('bookmarked');
        }
      };

      bookmarkBtn.addEventListener('click', async () => {
        if (!animeId) { alert('Error: Anime ID tidak ditemukan.'); return; }
        bookmarkBtn.disabled = true;
        const method = isBookmarked ? 'DELETE' : 'POST';
        const url = '/api/bookmarks';
        let fetchUrl = url;
        const options = { method: method, headers: { 'Content-Type': 'application/json' } };

        if (method === 'POST') {
          options.body = JSON.stringify({ userId: userId, animeId: animeId });
        } else {
          fetchUrl = `/api/bookmarks?userId=${userId}&animeId=${animeId}`;
          delete options.headers;
        }

        try {
          const response = await fetch(fetchUrl, options);
          if (!response.ok) throw new Error(`HTTP error! status: ${response.status}`);
          const result = await response.json();
          if (result.success) {
            isBookmarked = result.isBookmarked;
            updateButton();
          } else {
            alert(`Gagal ${method === 'POST' ? 'menambah' : 'menghapus'} bookmark: ${result.error || 'Unknown error'}`);
          }
        } catch (error) {
          console.error(`Error saat ${method === 'POST' ? 'tambah' : 'hapus'} bookmark:`, error);
          alert('Error: Gagal menghubungi server.');
        } finally {
          bookmarkBtn.disabled = false;
        }
      });

      checkStatus(); // Panggil cek status awal
    }); // Akhir DOMContentLoaded (Anime)
  </script>
<% } %>


<%# --- Skrip Halaman Bookmarks --- %>
<% if (page === 'bookmarks') { %>
  <script>
    document.addEventListener('DOMContentLoaded', async () => {
      const grid = document.getElementById('bookmark-grid');
      const loadingMessage = document.getElementById('loading-message');
      const clearBtn = document.getElementById('clearBookmarksBtn');
      const userId = localStorage.getItem('nekopoiUserId');

      if (!userId) {
        if (loadingMessage) loadingMessage.textContent = 'Anda belum memiliki bookmark. Coba bookmark anime di halaman detail.';
        if (clearBtn) clearBtn.style.display = 'none';
        return;
      }

      if (!grid || !loadingMessage || !clearBtn) {
        console.error("Elemen bookmark atau tombol clear tidak ditemukan.");
        return;
      }

      const loadBookmarks = async () => {
        loadingMessage.textContent = 'Memuat bookmark...';
        loadingMessage.style.display = 'block';
        grid.innerHTML = ''; // Kosongkan grid
        grid.appendChild(loadingMessage);
        clearBtn.style.display = 'none';

        try {
          const response = await fetch(`/api/my-bookmarks?userId=${userId}`);
          if (!response.ok) throw new Error(`HTTP error! status: ${response.status}`);
          const animes = await response.json();
          console.log("Bookmarks received:", animes);
          loadingMessage.style.display = 'none';

          if (animes.length === 0) {
            grid.innerHTML = '<p>Anda belum memiliki bookmark.</p>';
            clearBtn.style.display = 'none';
          } else {
            animes.forEach(anime => {
              if (!anime) return;
              const card = document.createElement('div');
              card.className = 'card';
              const imageUrl = anime.imageUrl || '/images/default.jpg';
              card.innerHTML = `
                <a href="/anime/${anime.pageSlug}">
                  <img src="${imageUrl}" alt="${anime.title || 'No Title'}">
                  <h3>${anime.title || 'No Title'}</h3>
                </a>
              `;
              grid.appendChild(card);
            });
            clearBtn.style.display = 'block';
          }
        } catch (error) {
           console.error('Gagal memuat bookmark:', error);
           loadingMessage.textContent = 'Gagal memuat bookmark. Coba lagi nanti.';
        }
      };

      clearBtn.addEventListener('click', async () => {
        if (!confirm('Apakah Anda yakin ingin menghapus semua bookmark? Aksi ini tidak bisa dibatalkan.')) {
          return;
        }
        clearBtn.disabled = true;
        clearBtn.textContent = 'Menghapus...';
        try {
          const response = await fetch(`/api/bookmarks/all?userId=${userId}`, { method: 'DELETE' });
          const result = await response.json();
          if (response.ok && result.success) {
            alert(`Berhasil menghapus ${result.deletedCount} bookmark.`);
            grid.innerHTML = '<p>Anda belum memiliki bookmark.</p>';
            clearBtn.style.display = 'none';
          } else {
            alert('Gagal menghapus bookmark: ' + (result.error || 'Unknown error'));
            clearBtn.disabled = false;
            clearBtn.innerHTML = '<i class="fa fa-trash"></i> Clear All Bookmarks';
          }
        } catch (error) {
          console.error('Error saat clear bookmarks:', error);
          alert('Terjadi kesalahan saat menghubungi server.');
          clearBtn.disabled = false;
          clearBtn.innerHTML = '<i class="fa fa-trash"></i> Clear All Bookmarks';
        }
      });

      loadBookmarks(); // Muat bookmark saat halaman dibuka
    }); // Akhir DOMContentLoaded (Bookmarks)
  </script>
<% } %>

